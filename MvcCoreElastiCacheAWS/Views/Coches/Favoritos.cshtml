@model List<Coche>

<h1>Coches Favoritos AWS ElastiCache</h1>

@if(Model != null)
{
    @foreach(Coche car in Model)
    {
        <div class="card" style="width: 18rem;">
            <img src="@car.Imagen" class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title text-warning">@car.Marca @car.Modelo</h5>
                <a asp-controller="Coches" asp-action="DeleteFavorito"
                    asp-route-idcoche="@car.IdCoche" class="btn btn-danger">
                    Delete favorito
                </a>
            </div>
        </div>
    }
}
else
{
    <h2 class="text-danger">No existen coches favoritos</h2>
}